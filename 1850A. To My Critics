#include<bits/stdc++.h>
using namespace std;
#define file() freopen("input.txt","r",stdin);freopen("output.txt","w",stdout);
#define endl "\n"
#define ll long long
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

int main()

{
    optimize();
    int tt;cin>>tt;
    while(tt--){
        int a ,b,c;
    cin >> a>> b>> c;
    if((a+b)>=10 || (b+c) >=10 || (c+a)>=10){
        cout<<"YES"<<endl;

    }
    else{
        cout<< "NO"<<endl;
    }
    }



	return 0;
}




