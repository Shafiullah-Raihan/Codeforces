///https://codeforces.com/problemset/problem/272/A
#include<bits/stdc++.h>
#define endl "\n";
#define ll long long
using namespace std;

int main()
{
    int n,a,sum(0),fn(0);
    cin>> n;
    for(int i=1;i<=n;i++){
        cin>>a;
        sum += a;
    }
    for(int i=1;i<=5;i++){
        if((sum+i)%(n+1)!=1){
            fn++;
        }
    }
    cout<< fn<< endl;

    return 0;

}

