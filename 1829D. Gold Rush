ll gcd ( ll a, ll b ) { return __gcd ( a, b ); }
ll lcm ( ll a, ll b ) { return a * ( b / gcd ( a, b ) ); }


int dx[] = { 0, 0, +1, -1, -1  +1, -1, +1 };
int dy[] = { +1, -1, 0, 0, -1, +1, +1, -1 };


bool isValid(int n,int m)
{   if(n==m) return true;
    if(n<m || n%3 != 0) return false;
    return isValid(n/3,m) || isValid(n*2/3,m);

}


int main()
{
    bismillah;

    int T;cin>>T;
    while(T--)
    {
        int n,m;
        cin>>n>>m;
        if(isValid(n,m)){
            cout<< "YES"<<endl;
        }
        else{
            cout<< "NO"<<endl;
        }


    }



    return 0;
}

