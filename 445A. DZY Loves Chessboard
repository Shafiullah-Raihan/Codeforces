#include<bits/stdc++.h>
using namespace std;

typedef vector<int> vi;
typedef pair<int,int> pi;
typedef vector<vector<int>> vvi;

#define fore(i,a,b) for(int i=a;i<=b;i++)
#define forl(i,a,b) for(int i=a;i<b;i++)
#define nl cout<<endl
#define F first
#define S second
#define pb push_back
#define mp make_pair
#define endl "\n"
#define ll long long
#define bismillah ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

int main() {
    bismillah;

    int n,m;
    cin>>m>>n;
    char c;
    char graph[m][n];
    forl(i,0,m)
    {
        forl(j,0,n)
        {
            cin>> graph[i][j];
            if(graph[i][j] == '.'){
                if((i+j)%2==0){
                    graph[i][j] = 'B';
                }
                else{
                    graph[i][j] = 'W';
                }
            }
        }

    }
            forl(i,0,m){
        forl(j,0,n)
        {
            cout<< graph[i][j];
        }
        nl;
        }



    return 0;
}


























