///https://codeforces.com/problemset/problem/450/A
#include<bits/stdc++.h>
#define endl "\n";
#define ll long long
using namespace std;

int main()
{
    int m,n,p,last,mx(0);
    double t;
    cin>> n>> m;
    for(int i=1;i<=n;i++){
        cin>>t;
        if(ceil(t/m)>=mx){
            p = i;
            last = t;
            mx= ceil(t/m);
        }


    }
    cout<< p<< endl;

    return 0;

}
