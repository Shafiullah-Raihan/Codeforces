///https://codeforces.com/problemset/problem/58/A
#include<bits/stdc++.h>
using namespace std;
int main()
{

    string s,s1="hello";
    cin>> s;
    int k=0;
    for(int i=0;i<s.size();i++){
        if(s1[k]==s[i]){
            k++;
        }
    }
    if(k==s1.size()){
        cout<< "YES"<< endl;
    }
    else{cout<< "NO"<< endl;}
}

