///https://codeforces.com/problemset/problem/271/A
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int y;
    cin>> y;
    while(1){
        y+=1;
        int a = y/1000;
        int b = (y/100)%10;
        int c = (y%100)/10;
        int d = (y%10);
        if(a!=b && b!=c && c!=d && d!=a && a!=c && b!=d){

            break;
        }



    }
    cout << y << endl;


}
