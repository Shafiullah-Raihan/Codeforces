#include<bits/stdc++.h>
using namespace std;

#define bismillah ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);





int main()
{
    bismillah;
    int T;cin>>T;
    while(T--)
    {
        int n;
        cin>>n;
        if(n%2==1) {
            cout<< "-1"<<endl;
            continue;

        }

            for(int i=1;i<=n;i+=2){
                cout<< i <<" "<<i << " " ;
            }
            cout<<endl;






    }
    return 0;
}

